<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pankaj.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SpinnerTest"
            android:label="@string/title_activity_spinner"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".SendIntentTest"
            android:label="@string/title_activity_send_intent_test"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".GetIntentTest"
            android:label="@string/title_activity_get_intent_test"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".ColorButtons"
            android:label="@string/title_activity_color_buttons"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".LifeCycle"
            android:label="@string/title_activity_life_cycle"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".RadioButton"
            android:label="@string/title_activity_radio_button"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
        <activity
            android:name=".SQLiteTest"
            android:label="@string/title_activity_sqlite_test"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.pankaj.helloworld.MainActivity" />
        </activity>
    </application>

</manifest>